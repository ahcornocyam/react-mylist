sssimport React, {Component} from 'react'

class ListItem extends Component {
  state = {
    totalClicks : 0
  }
  static defaultProps = {}
  constructor(props) {
    super(props)
  }

  clicks() {
    let totalClicks = ++this.state.totalClicks
    this.state = {totalClicks}
  }
render () {
  let props = this.props
  let state = this.state
  return (
    <li onClick={this.clicks} >
      {props.children} - {state.totalClicks}
    </li>
      )
}
}

export default ListItem
